 spring:
  datasource:
    url: jdbc:mysql://localhost:3306/rakuten_db?useSSL=false&serverTimezone=Asia/Tokyo&allowPublicKeyRetrieval=true
    username: user
    password: pass
    driver-class-name: com.mysql.cj.jdbc.Driver
  jpa:
    hibernate:
      ddl-auto: update
    show-sql: true
    properties:
      hibernate:
        dialect: org.hibernate.dialect.MySQLDialect
